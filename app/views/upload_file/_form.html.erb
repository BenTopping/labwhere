<%= form_for @upload_file, url: upload_file_index_path do |f| %>
  <fieldset>
    <legend>Upload File</legend>
    <ul>
      <li>
        <%= render "user_code", object: f %>
      </li>
      <li>
        <div><%= f.label :file, 'Upload a file here*' %></div>
        <div><%= f.file_field :file %></div>
      </li>
      <li>
        <%= f.submit "Go!", data: {disable_with: "uploading..."} %>
      </li>
    </ul>
  </fieldset>
<% end %>
